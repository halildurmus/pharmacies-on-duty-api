    "got": "^10.7.0",
    "hbs": "^4.1.1",
    "http-status": "^1.4.2",
    "ioredis": "^4.17.3",
    "node-cache": "^5.1.2",
    "url": "^0.11.0",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "concurrently": "^5.3.0",
    "cross-env": "^7.0.2",
    "eslint": "^7.7.0",
